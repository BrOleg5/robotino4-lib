# Config file for the Robotino4 package
# It defines the following variables
#  Robotino4_INCLUDE_DIRS - include directories for Robotino4
#  Robotino4_LIBRARIES    - libraries to link against
#  Robotino4_EXECUTABLE   - the tests executable

# Compute paths
get_filename_component(ROBOTINO4_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(Robotino4_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET Robotino4Lib AND NOT Robotino4_BINARY_DIR)
    include("${ROBOTINO4_CMAKE_DIR}/Robotino4Targets.cmake")
endif()

# These are IMPORTED targets created by Robotino4Targets.cmake
set(Robotino4_LIBRARIES Robotino4Lib)
if(@INSTALL_TESTS@)
    set(Robotino4_EXECUTABLE main_test)
endif()